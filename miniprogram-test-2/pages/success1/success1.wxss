.content{
  width: 100%;
  height: 336rpx;
   background:linear-gradient(180deg,rgba(145,229,115,1) 0%,rgba(61,178,163,1) 100%);
   text-align: center;
   position: relative;
}
.content>.succ{
  width: 200rpx;
  height: 40rpx;
  margin: 10rpx auto 30rpx auto;
  padding-top: 20rpx;
  box-sizing: border-box;
}
.content>.succ>image{
  width: 40rpx;
  height: 40rpx;
}
.content>.succ>text{
  font-size:36rpx;

font-weight:bold;
color:rgba(255,255,255,1);
margin-left: 15rpx;
}
.content>text{
font-size:28rpx;

font-weight:500;
color:rgba(255,255,255,1);
}
