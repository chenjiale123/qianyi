.content>.btn{
  width: 80%;
  height: 68rpx;
  margin: 56rpx auto 0 auto;
  
}
.content>.btn>.left{
  width: 228rpx;
  height: 68rpx;
  float: left;

border-radius:33rpx;
border:2px solid rgba(255,255,255,1);
line-height: 68rpx;
color: #fff;
text-align: center;
font-size:36rpx;
background: none
}
.content>.btn>.right{
  width: 228rpx;
  height: 68rpx;
  float: right;

border-radius:33rpx;
border:2px solid rgba(255,255,255,1);
line-height: 68rpx;
color: #fff;
text-align: center;
font-size:36rpx;
background: none

}
.bj{
  width: 100%;
  position: absolute;
  bottom:0;
  left: 0;
  height: 88rpx;
}
.bj>image{
  width: 100%;
  height: 100%;
}
.love{
  text-align: center;
  width: 100%;
  height: auto;
  margin-top: 50rpx;

}
.love>text{
  font-size:36rpx;

color:rgba(26,26,26,1);
}
.love>image{
  display: block;
  width: 142rpx;
  height: 10rpx;
  text-align: center;
  position: relative;
  top: -5rpx;
  margin: 0 auto;

}
.love>.li{
  width: 336rpx;
  height: 526rpx;
  float: left;
  background:rgba(255,255,255,1);
box-shadow:0px 6rpx 12rpx 0px rgba(0, 0, 0, 0.08);
border-radius:10rpx;
text-align: left;
margin-left: 13rpx;
margin-top: 30rpx;
}
.love>.li>image{
    width: 336rpx;
  height: 336rpx;
}
.love>.li>text{
  font-size:26rpx;

color:rgba(26,26,26,1);
}
